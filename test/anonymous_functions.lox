fun thrice(fn) {
  for (var i = 1; i <= 3; i = i + 1) {
    fn(i);
  }
}

var a = fun (a) {
  print(a);
};

var b = fun (b) {
  print(b + 3);
};

thrice(a);
// "1".
// "2".
// "3".

thrice(b);
// "4".
// "5".
// "6".

// This one's tricky: anonymous function
// occurring in an expression statement.
fun () {};